<div class="row">
    <div class="card-deck">
{{#each products }}
        {{#each this}}
        <div class="col-4  card-bottom">
        <div class="card">
            <img class="card-img-top" src="{{ this.imagePath }}" alt="Card image cap">
            <div class="card-body">
                <h5 class="card-title"><span>{{ this.title }}<br> <b>{{this.price}}$</b> </span></h5>
            <p class="card-text">{{ this.description }}</p>
            <button class="btn btn-primary">BUY</button>
            <button class="btn btn-success">Wishlist</button>
            </div>
        </div>
        </div>

            {{/each}}
{{/each }}
</div>
</div>
